<head>
    <link href="/css/listinfo.css" rel="stylesheet">
</head>

<%- include('m_header'); %>

<div class="staff_list_wrapper">
<p class="staffs_list_title">スタッフ 一覧</p>
<div class="staff_list">
    <div class="staff_list_in">
        <% staffList.forEach(staffList => { %>
        <ul class="staff_list_ul">
            <li id="staff_list_inner">
                <a class="staff_list_name" href="<%=`/managers/staffs/${staffList.staff_id}`%>">
                    <%= staffList.staff_name %></a>
            </li>
        </ul>
        <% }); %>
    </div>
</div>
</div>